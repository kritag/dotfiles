{
    "label" : "lock",
    "action" : "$HOME/.config/hypr/scripts/hyprlock.sh",
    "keybind" : "l"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "keybind" : "r"
}
{
    "label" : "logout",
    "action" : "loginctl terminate-user $USER",
    "keybind" : "e"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "keybind" : "s"
}
